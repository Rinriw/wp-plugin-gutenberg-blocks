{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "acf/gallery",
  "title": "Gallery",
  "description": "Galería de imágenes responsive con carrusel en mobile y grid en desktop",
  "category": "acf-blocks",
  "icon": "format-gallery",
  "keywords": ["galería", "gallery", "imágenes", "images", "carrusel", "carousel"],
  "acf": {
    "mode": "preview",
    "renderTemplate": "render.php",
    "blockVersion": 3
  },
  "supports": {
    "align": ["wide", "full"],
    "anchor": true,
    "customClassName": true
  },
  "example": {
    "attributes": {
      "mode": "preview"
    }
  }
}
